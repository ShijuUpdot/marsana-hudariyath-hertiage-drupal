                                <a target="_blank" href="{{ item['landmarkUrl'] }}"><p>{{ item['landmarkName'] | raw }}</p>
                                <img src="/images/static/arrow-right-navy.svg" alt=""></a>
                            </div>
                        </div>
                    {% else %}
                        <a class="marker-all map-pin {{  item['cssClass']|trim }} active" style="{{ item['landmarkPosition'] }}" data-tag="{{ item['landmarkName']|replace({'&#039;':"-", '&amp;' : '&'})|capitalize }}">
							<div class="pulse"></div>
                            <p class="marker-details tooltip-txt">{{ item['landmarkName'] }}</p>
                        </a>
                    {% endif %}
                {% endif %}
            {% endfor %}
            {% for row in rows %}
                {% set title   = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_hero_image_title }}'] | striptags | trim %}
                {% set aminitesType = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_amenity_type }}'] | striptags | trim %}
                {% set hero_image = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_hero_image }}'] | striptags | trim %}
                {% set link = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ view_node }}'] | striptags | trim  %}
                {% set position = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_position }}'] | striptags | trim %}
                {% set aminityTag = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_amenity_tags }}'] | striptags | trim %}
                {% set suggestions = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_autosuggestion_keywords }}'] | striptags | trim %}
	            {% set isRecommended = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_is_recommended }}'] | striptags | trim %}
                {% set suggestionTag = '' %}
                {% set isRecommend = '' %}

		{% set allSuggs = suggestions | trim | replace({", ":","," ,":",",'&#039;':"-", '&amp;' : '&'}) | split(",") %}
                {% set allCapSuggs = [] %}
                {% for sugg in allSuggs %}
                    {% set allCapSuggs = allCapSuggs | merge([sugg|capitalize]) %}
                {% endfor %}
                {% set suggestionTag = allCapSuggs | join(',') %}   

                {% if isRecommended == 'Yes' %}
                    {% set isRecommend = ',recommend'%}
                {% endif %}

                {% if position %}
			{% if aminitesType == 'Restaurants' or aminitesType == 'Food Trucks' or aminitesType == 'مطاعم' or aminitesType == 'شاحنات الغذاء' %}
                        <a class="marker-all map-pin green-pin active" style="{{ position }}" href="javascript:;" data-toggle="modal" data-target="#{{ title|raw|replace({" ":"-",".":"-", '&#039;':"-", '&amp;' : '-'})|lower }}" data-tag="{{ title|replace({'&#039;':"-", '&amp;' : '&'})|capitalize }},{{ aminitesType|replace({" ":"-", ".":"-", '&#039;':"-", '&amp;' : '-'})|lower }},{{ aminityTag|replace({" ":"-", ".":"-", '&#039;':"-", '&amp;' : '-'})|lower }},{{ suggestionTag|trim }}{{ isRecommend }}">
                            <div class="pulse"></div>
                            <p class="marker-details tooltip-txt">{{ title | raw }}</p>
                        </a>
		    {% elseif(aminitesType == 'Facilities' or aminitesType == 'Activities' or aminitesType == 'رياضات' or aminitesType == 'المنشآت') %}
                        <div class="redirection marker-all map-pin navy-pin active" style="{{ position }}" data-tag="{{ title|replace({'&#039;':"-", '&amp;' : '&'})|capitalize }},{{ aminitesType|replace({" ":"-", '&#039;':"-", '&amp;' : '-'})|lower }},{{ suggestionTag|trim }}{{ isRecommend }},{{ aminityTag|trim|replace({" ,":",",", ":","," ":"-", ".":"-", '&#039;':"-", '&amp;' : '-'})|lower }}">
