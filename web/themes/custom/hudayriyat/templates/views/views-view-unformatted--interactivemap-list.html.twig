        <div class="map-control map-control-reset"></div>
        <div class="map-control map-control-zoomout"></div>
        <div class="map-control map-control-zoomin"></div>
    </div>
</div>

<!------------------------------------  RESTAURANTS MODALS START HERE ------------------------------>

<ul class="modal-listing-wrapper interactive-modal-listing">
    {% for row in rows %}
        {% set logo = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_amenity_icon }}'] | striptags | trim %}
        {% set title = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_hero_image_title }}'] | striptags | trim %}
        {% set assetSubType = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_amenity_tags }}'] | striptags | trim %}
        {% set hero_image = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_hero_image }}'] | striptags | trim %}
        {% set desc = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_intro_description }}'] | striptags | trim %}
        {% set position = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_position }}'] | striptags | trim %}
        {% set sub_desc = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_intro_short_description }}'] | striptags | trim %}
        {% set pdf_url = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_upload }}'] | striptags | trim %}
        {% set gallery = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_amenity_images }}']|striptags|trim|split(',') %}
        {% set images_title = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_amenity_images_title }}'] | striptags | trim %}
        {% set aminitesType = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_amenity_type }}'] | striptags | trim %}
        {% if position %}
	    {% if aminitesType == 'Restaurants' or aminitesType == 'Food Trucks' or aminitesType == 'مطاعم' or aminitesType == 'شاحنات الغذاء' %}
        <li class="modal fade" id="{{ title|replace({" ":"-", ".":"-", '&#039;':"-", '&amp;' : '-'})|lower }}" tabindex="-1" role="dialog" aria-labelledby="footballLabel"  aria-hidden="true">
        <div class="inside-wrapper modal-wrapper">
            <div class="container">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                <div class="modal-header">                          
                    <div class="modal-header-bg navy-bg">
                        {% if hero_image %}
                        <img src="{{ hero_image }}" alt="">
                        {% endif %}
                    </div>                          
                    <div class="modal-header-wrapper">                            
                        <div class="modal-logo-wrapper">
                        {% if logo %}
                            <img class="modal-logo-top" src="{{ logo }}" alt="Resto logo">
                        {% endif %}
                        </div>                            
                    <div class="modal-header-content">
                        <h3 class="modal-title" id="footballLabel">{{ title | raw }}</h3>
                    </div>
                    </div>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true"><img src="/images/static/close.svg" alt="close"></span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="container-fluid">
                    <div class="row">
                        {{ row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_info_block }}'] }}
                    </div>
                    <div class="row py-4">
                        <div class="col-lg-4">
                        <p class="detailed-txt bold detailed-desc">{{ sub_desc | raw  }}</p>
                        </div>
                        <div class="col-lg-8">
                        <p class="detailed-txt detailed-desc"> {{ desc | raw  }}</p>
                        </div>
                    </div>
                    </div>
                </div>
                {% if gallery[0] %}
                <div class="modal-footer swiper-menu">
                    <div class="modal-footer-title">{{ images_title }}</div>
                    <!-- Swiper -->
                    <div class="swiper-container">
                    <div class="swiper-wrapper">
                    {% for gkey, item in gallery %}
                        <div class="swiper-slide">
                            <div class="card-wrapper">
                                <div class="card event">
                                    <div class="card-img-wrapper">
                                        <img class="card-img-top" src="{{ gallery[gkey] }}" alt="Card image cap">
                                    </div>
                                </div>
                            </div>
                        </div>
                    {% endfor %}                    
                    </div>
                    <div class="swiper-arrows">
                        <div class="swiper-button-prev"></div>
                        <div class="swiper-button-next"></div>
                    </div>
                    </div>
                </div>
                {% endif %}
                </div>
            </div>
            </div>
        </div>
    </li>
    {% endif %}
    {% endif %}
    {% endfor %}
  </ul>

