{#
/**
 * @file
 * Default theme implementation to display a view of unformatted rows.
 *
 * Available variables:
 * - title: The title of this group of rows. May be empty.
 * - rows: A list of the view's row items.
 *   - attributes: The row's HTML attributes.
 *   - content: The row's content.
 * - view: The view object.
 * - default_row_class: A flag indicating whether default classes should be
 *   used on rows.
 *
 * @see template_preprocess_views_view_unformatted()
 *
 * @ingroup themeable
 */
#}
{% if langcode == 'en' %}
    {% set recommendation = drupal_config('hudayriyat_custom_configs.settings', 'custom_configs_recommendation_en')  %}
  {% else %}
    {% set recommendation = drupal_config('hudayriyat_custom_configs.settings', 'custom_configs_recommendation_ar') %}
  {% endif %}
<div class="interactive-map">
<div class="map">
    <div class="maps-container unselectable maps-container-mob" style="overflow: auto;">

    <div class="map-mobile" style="transform: scale(1); transform-origin: 0 0;">
            <div class="map-image" id="map-all">
              <div id="animation_container">
                    {{ drupal_view('interactivemap_map_section') | render }}
                    <div class="trails-group">
                    {# {% for tkey,trailItem in trails %}
                            <img src="{{ trailItem['image_url'] }}" alt="" data-tag="{{ trailItem['trailsName']|replace({" ":"-", ".":"-", '&#039;':"-", '&amp;' : '-'})|lower }}" class="active">
                    {% endfor %} #}
                    </div>
                    <div id="dom_overlay_container" style="pointer-events:none; overflow:hidden; width:100%; height:auto; position: absolute; left: 0px; top: 0px; display: block;">
                </div>
              </div>
            </div>
            
            <div class="marker-group">
              
              {% for key,item in landmarks %}
               {% if item['landmarkType'] and item['landmarkType'] == 'Landmarks' %}
                {% if item['landmarkImage'] and item['landmarkUrl'] %}
                    <div class="redirection marker-all {{ item['cssClass'] }}" style="{{ item['landmarkPosition'] }} transform: scale(1);" data-tag="{{ item['landmarkName']|capitalize }}">
						<div class="pulse"></div>
                        <a href="javascript:;" class="pin-redirection"></a>
                        <a href="javascript:;">
                            <p class="marker-details tooltip-txt">{{ item['landmarkName'] }}</p>
                        </a>
                        <span>{{ item['landmarkNo'] }}</span>                    
                        <div class="marker-details redirection-details">
                            <img src="{{ item['landmarkImage'] }}" alt="" class="w-100">
                            <a href="javascript:;" class="redirection-details-close"><img src="/images/static/close.svg" alt=""></a>
                            <a target="_blank" href="{{ item['landmarkUrl'] }}"><p>{{ item['landmarkName'] }}</p>
                            <img src="/images/static/arrow-right-navy.svg" alt=""></a>
                        </div>
                    </div>
                {% else %}
                    <a class="marker-all {{ item['cssClass'] }}" style="{{ item['landmarkPosition'] }}" href="javascript:;" data-tag="{{ item['landmarkName']|capitalize }}">
						<div class="pulse"></div>
                        <span class="number">{{ item['landmarkNo'] }}</span>
                        <p class="marker-details tooltip-txt">{{ item['landmarkName'] }}</p>
                    </a>
                {% endif %}
               {% endif %}

                {% if item['landmarkType'] and item['landmarkType'] != 'Landmarks' %}
                    {% if item['landmarkImage'] and item['landmarkUrl'] %}
                        <div class="redirection marker-all map-pin {{ item['cssClass'] }} active" style="{{ item['landmarkPosition'] }} transform: scale(1);" data-tag="{{ item['landmarkName']|replace({'&#039;':"-", '&amp;' : '&'})|capitalize }}">
                            <div class="pulse"></div>
                            <a class="pin-redirection" href="javascript:;"></a>
                            <a class="" href="javascript:;">
                                <p class="marker-details tooltip-txt">{{ item['landmarkName'] | raw }}</p>
                            </a>
                            <div class="marker-details redirection-details">
                                <img src="{{ item['landmarkImage'] }}" alt="{{ item['landmarkName'] | raw }}">
                                <a class="redirection-details-close" href="javascript:;">
                                    <img src="/images/static/close.svg" alt="" />
                                </a>
                                <a target="_blank" href="{{ item['landmarkUrl'] }}"><p>{{ item['landmarkName'] | raw }}</p>
                                <img src="/images/static/arrow-right-navy.svg" alt=""></a>
                            </div>
                        </div>
                    {% else %}
                        <a class="marker-all map-pin {{ item['cssClass'] |trim }} active" style="{{ item['landmarkPosition'] }}"  data-tag="{{ item['landmarkName']|replace({'&#039;':"-", '&amp;' : '&'})|capitalize }}">
							<div class="pulse"></div>
                            <p class="marker-details tooltip-txt">{{ item['landmarkName'] }}</p>
                        </a>
                    {% endif %}
                {% endif %}
              {% endfor %}

              {% for row in rows %}
                {% set title   = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_hero_image_title }}'] | striptags | trim %}
                {% set aminitesType = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_amenity_type }}'] | striptags | trim %}
                {% set hero_image = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_hero_image }}'] | striptags | trim %}
                {% set link = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ view_node }}'] | striptags | trim  %}
                {% set position = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_position }}'] | striptags | trim %}
                {% set aminityTag = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_amenity_tags }}'] | striptags | trim %}
                {% set suggestions = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_autosuggestion_keywords }}'] | striptags | trim %}
	            {% set isRecommended = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_is_recommended }}'] | striptags | trim %}
                {% set suggestionTag = '' %}
                {% set isRecommend = '' %}

		        {% set allSuggs = suggestions | trim | replace({", ":","," ,":",",'&#039;':"-", '&amp;' : '&'}) | split(",") %}
                {% set allCapSuggs = [] %}
                {% for sugg in allSuggs %}
                    {% set allCapSuggs = allCapSuggs | merge([sugg|capitalize]) %}
                {% endfor %}
                {% set suggestionTag = allCapSuggs | join(',') %}   

                {% if isRecommended == 'Yes' %}
                    {% set isRecommend = ',recommend'%}
                {% endif %}

                {% if position %}
                    {% if aminitesType == 'Restaurants' or aminitesType == 'Food Trucks' or aminitesType == 'مطاعم' or aminitesType == 'شاحنات الغذاء' %}
                        <a class="marker-all map-pin green-pin active" style="{{ position }} transform: scale(1);" href="javascript:;" data-toggle="modal" data-target="#{{ title|raw|replace({" ":"-",".":"-", '&#039;':"-", '&amp;' : '-'})|lower }}" data-tag="{{ title|replace({'&#039;':"-", '&amp;' : '&'})|capitalize }},{{ aminitesType|replace({" ":"-", ".":"-", '&#039;':"-", '&amp;' : '-'})|lower }},{{ aminityTag|replace({" ":"-", ".":"-", '&#039;':"-", '&amp;' : '-'})|lower }},{{ suggestionTag|trim }}{{ isRecommend }}">
                            <div class="pulse"></div>
                            <p class="marker-details tooltip-txt">{{ title | raw }}</p>
                        </a>
                    {% elseif(aminitesType == 'Facilities' or aminitesType == 'Activities' or aminitesType == 'النشاطات' or aminitesType == 'المنشآت') %}
                        <div class="redirection marker-all map-pin navy-pin active" style="{{ position }} transform: scale(1);" data-tag="{{ title|replace({'&#039;':"-", '&amp;' : '&'})|capitalize }},{{ aminitesType|replace({" ":"-", '&#039;':"-", '&amp;' : '-'})|lower }},{{ suggestionTag|trim }}{{ isRecommend }},{{ aminityTag|trim|replace({" ,":",",", ":","," ":"-", ".":"-", '&#039;':"-", '&amp;' : '-'})|lower }}">
                            <div class="pulse"></div>
                            <a class="pin-redirection" href="javascript:;"></a>
                            <a class="" href="javascript:;">
                                <p class="marker-details tooltip-txt">{{ title | raw }}</p>
                            </a>
                            <div class="marker-details redirection-details">
                                <img src="{{ hero_image }}" alt="{{ title | raw }}">
                                <a class="redirection-details-close" href="javascript:;">
                                    <img src="/images/static/close.svg" alt="" />
                                </a>
                                <a target="_blank" href="{{ link }}"><p>{{ title | raw }}</p>
                                <img src="/images/static/arrow-right-navy.svg" alt=""></a>
                            </div>
                        </div>
                {% endif %}
               {% endif %}
            {% endfor %}
            </div>
        </div>
        

    <!-- maps-container-inner -->
    <div class="maps-container-inner zoomedElement zoomedElement414">
        <!-- maps-zoomed-container -->
        <div class="maps-zoomed-container">

        <div class="map-image" id="map-all">
