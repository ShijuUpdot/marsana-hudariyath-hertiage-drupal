<div class="recommendation" style='display:none;'>
    <img src="/images/static/search-navy.svg" alt="Search Icon" class="search-img">
    <p class="w-80">{{ recommendation }}</p>
    <a href="javascript:;" class="re-close"><img src="/images/static/close-navy.svg" alt="Search Icon" class="w-100"></a>
</div>
<div class="inside-wrapper">
    <div class="inside ">
    <div class="search">
        <div class="related-content related-interaction white-bg">
        <div class="container-fluid">
            <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
            <div class="panel panel-default">
                <div class="row justify-content-end">
                <div class="col-lg-8 py-4 order-2 order-md-1">
                    <div class="input-group">
                        <input type="text" class="form-control" placeholder="{{ 'What are you looking for?'|t }}" aria-label="Search field" aria-describedby="" id="search-text">
                    </div>
                    <ul class="input-suggestions interactive-tags py-4" style="display: none;">
                        {% for skey,sitem in autosuggest %}
                            {% if sitem != '' %}
                                <li><a href="javascript:;" class="" data-tag="{{ sitem|raw|trim|capitalize }}">{{ sitem|trim }}</a></li>
                            {% endif %}
                        {% endfor %}
                    </ul>                  
                    <ul class="interactive-rec-tags py-4" style="display: none;">
                    {% for row in rows %}
                        {% set isRecommended = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_is_recommended }}'] | striptags | trim %}
                        {% if isRecommended == 'Yes' %}
                            <li><a href="javascript:;" class="" data-tag="{{ 'recommend' }}"></a>{{ 'Recommended' }}</li>
                        {% endif %}
                    {% endfor %}
                    </ul>
                </div>
                <div class="col-lg-3 order-1 order-md-2 panel-heading navy-bg" role="tab" id="headingOne">
                    <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="false" aria-controls="collapseOne">
                        {{ 'Legend' |t }} 
                    </a>
                </div>
                </div>
                <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
                <div class="row justify-content-end">
                    <div class="col-11 overflow-auto">
                    <ul class="tags-listing trails-listing">
                    {% for trkey,trailsItem in trails %}
                        <li>
                        <div class="form-check {{ trailsItem['color'] }} form-check-inline" data-tag="{{ trailsItem['trailsName']|replace({" ":"-", ".":"-", '&#039;':"-", '&amp;' : '-'})|lower }}">
                            <input class="form-check-input" type="checkbox" name="flexRadioDefault" id="inlineCheckbox{{ trkey }}" value="option{{ trkey }}">
                            <label class="form-check-label" for="inlineCheckbox{{ trkey }}">
                            {{ trailsItem['trailsTitle'] }}
                            </label>
                        </div>
                        </li>
                    {% endfor %}
                    </ul>
                    </div>
                </div>
                <div class="row justify-content-center no-border">
                    <div class="col-10">
			{% set newAmty = [] %}
                        <ul class="tags-listing interactive-tags">
                            {% for row in rows %}
                                {% set aminitesType = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_amenity_type }}'] | striptags | trim %}
                                {% set position = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_position }}'] | striptags | trim %}
                                {% set aminityTag = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_amenity_tags }}'] | striptags | trim %}

                                {% if position != "" %}
                                {% set amtag = aminityTag ~"," ~aminitesType %}
                                {% set attags = amtag|split(",") %}
                               {% for attagkey,tag in attags %}
                                    {% if tag | trim not in newAmty %}
                                    {% set newAmty = newAmty|merge([tag | trim]) %}
                                    {% endif %}
                               {% endfor %}
                            {% endif %}
                            {% endfor %}
                            {% for newTag in newAmty %}
                            {% if newTag != '' %}
                    		<li><a href="javascript:;" class="white-btn {{ newTag }}" data-tag="{{ newTag|trim|replace({" ":"-", ".":"-", '&#039;':"-", '&amp;' : '-'})|lower }}">{{ newTag|trim }}</a></li>
                    	    {% endif %}
                	 {% endfor %}
                        </ul>
                    </div>
                </div>
                <div class="mb-2 col-12 col-md-11 interactive-submit">
                    <a href="javascript:;" class="btn border-btn border-yellow-btn">{{ 'Submit'|t }}</a>
                </div>
                <div class="row">
                </div>
                </div>
            </div>
            </div>
        </div>
        </div>
    </div>
    </div>
<!------------------------------------  END INSIDE SECTIONS ------------------------------>

</div>
