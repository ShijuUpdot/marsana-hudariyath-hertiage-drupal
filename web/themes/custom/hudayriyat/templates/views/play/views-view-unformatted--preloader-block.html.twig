{#
/**
 * @file
 * Default theme implementation to display a view of unformatted rows.
 *
 * Available variables:
 * - title: The title of this group of rows. May be empty.
 * - rows: A list of the view's row items.
 *   - attributes: The row's HTML attributes.
 *   - content: The row's content.
 * - view: The view object.
 * - default_row_class: A flag indicating whether default classes should be
 *   used on rows.
 *
 * @see template_preprocess_views_view_unformatted()
 *
 * @ingroup themeable
 */
#}
{% if cookie == '' %}

{% for row in rows %}
{% set field_preloader_content = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_preloader_content }}'] %}
{% set field_preloader_discover_label = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_preloader_discover_label }}'] %}
{% set field_preloader_bottom_links = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_preloader_bottom_links }}'] | split(',') %}
{% set field_preloader_social_links = row.content['#view'].style_plugin.render_tokens[ loop.index0 ]['{{ field_preloader_social_links }}'] %}


<div class="height-100 blue-bg" id="preloader">
  <header>
    <div class="container">
      <div class="row">
        <div class="col-3 col-md-4 col-lg-5 my-auto">
        </div>
        <div class="col-6 col-md-4 col-lg-2 text-center my-auto z-100">
          <a href="/{{ langclass }}/"><img src="/themes/custom/hudayriyat/logo.svg" alt="Logo" class="w-100"></a>
        </div>
        <div class="col-3 col-md-4 col-lg-5 my-auto z-100">
        </div>
      </div>
    </div>
  </header>
  <section>
    <div class="lp">
      <div class="lp-wrapper container">
        <h1 class="onLoadAnimation load-one">{{ field_preloader_content }}</h1>
        <div class="btns fadeIn load-two">
          <div class="progress dashed-line">
            <div id="dynamic" class="progress-bar progress-bar-success active" role="progressbar" aria-valuenow="0"
              aria-valuemin="0" aria-valuemax="100" style="width: 0%">
              <span id="current-progress"></span>
            </div>
          </div>
          <img src="/images/static/line.svg" alt="Dashed line" class="dashed-line">
          <a href="javascript:;" onclick="showPageDirectly()" class="btn white-btn discover-btn">{{ field_preloader_discover_label }}</a>
        </div>
        <div class="illustration-img-holder onLoadAnimation load-three"></div>
      </div>
    </div>
  </section>
  <section>
    <div class="lp-footer">
      <div class="container">
        <div class="row align-self-center">
          <div class="col-md-6">
            <ul class="nav-secondary">
              {% for key, bottomlink in field_preloader_bottom_links %}
              <li>{{ bottomlink | raw }}</li>
              {% endfor %}
            </ul>
          </div>
          <div class="col-md-6">
            <ul class="social">
              {{ field_preloader_social_links }}
            </ul>
          </div>
        </div>
      </div>
    </div>
  </section>
</div>
{% endfor %}
{% endif %}
