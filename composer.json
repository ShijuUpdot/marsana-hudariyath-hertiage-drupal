{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "drupal/addtoany": "^2.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/advagg": "^6.0@alpha",
        "drupal/antibot": "^2.0",
        "drupal/az_blob_fs": "^2.3",
        "drupal/backup_migrate": "^5.0",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/block_visibility_groups": "^2.0",
        "drupal/ckeditor_config": "^3.4",
        "drupal/color_field": "^3.0",
        "drupal/config_partial_export": "^2.0@alpha",
        "drupal/core-composer-scaffold": "^10.2",
        "drupal/core-project-message": "^10.2",
        "drupal/core-recommended": "^10.2",
        "drupal/core-vendor-hardening": "^10.2",
        "drupal/custom_body_class": "^2.0",
        "drupal/devel": "^5.2",
        "drupal/entity_type_clone": "^4.0",
        "drupal/flag": "^4.0@beta",
        "drupal/geofield": "^1.57",
        "drupal/honeypot": "^2.1",
        "drupal/hreflang": "^1.14",
        "drupal/mailsystem": "^4.4",
        "drupal/message": "^1.5",
        "drupal/message_notify": "^1.3",
        "drupal/message_ui": "^1.2",
        "drupal/metatag": "^2.0",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/multiple_fields_remove_button": "^2.2",
        "drupal/paragraphs": "^1.17",
        "drupal/paragraphs_collapsible": "^1.6",
        "drupal/phpmailer_smtp": "^2.2",
        "drupal/rdf": "^2.0",
        "drupal/recaptcha_v3": "^2.0",
        "drupal/redirect_after_login": "^3.0",
        "drupal/redis": "^1.7",
        "drupal/restui": "^1.21",
        "drupal/schema_metatag": "^3.0",
        "drupal/search_api": "^1.34",
        "drupal/search_api_location": "^1.0@alpha",
        "drupal/simple_sitemap": "^4.1",
        "drupal/smtp": "^1.2",
        "drupal/svg_image": "^3.0",
        "drupal/token": "^1.14",
        "drupal/twig_tweak": "^3.3",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/upgrade_status": "^4.3",
        "drupal/view_custom_table": "^2.0",
        "drupal/views_custom_cache_tag": "^1.3",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/webform": "^6.2",
        "drush/drush": "^13.3"
    },
    "require-dev": {
        "drupal/core-dev": "^10.2",
        "palantirnet/drupal-rector": "^0.20.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "php-http/discovery": true,
            "drupal/core-vendor-hardening": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "enable-patching": true,
        "patches":{
        "drupal/core": {
          "Support REST multipart/form-data uploads": "https://www.drupal.org/files/issues/2018-11-20/rest_multipart_upload-3015154-2.patch",
          "Introduce a token to get site's base URL": "https://www.drupal.org/files/issues/2020-10-06/1088112-63.patch"
        },
        "guzzlehttp/guzzle": {
                                "Deprecated function: Return type of GuzzleHttp": "./CookieJar2.patch"
                                            }
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
